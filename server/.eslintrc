{
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "rules": {
      "linebreak-style": 0,
      "no-console": 0,
      "no-shadow": 0,
      "global-require": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "import/prefer-default-export": 0
  },
  "env": {
      "jest": true
  }
}